/*

enter number of row :5

1
22
333
4444
55555
4444
333
22
1


*/



#include <stdio.h>
int main() {
    int row,i,j ;
    
    printf("enter number of row :\n");
    scanf("%d",&row);
    
    for(i=1;i<=row;i++)
    {
    	for(j=1;j<=i;j++)
    	{
    		printf("%d",i);
		}
		printf("\n");
	}
    

    for(i=row-1;i>=1;i--)
    {
    	for(j=1;j<=i;j++)
    	{
    		printf("%d",i);
		}
		printf("\n");
	}
  
}

